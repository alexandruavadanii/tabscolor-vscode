{
	"name": "tabscolor",
	"displayName": "Tabs color",
	"description": "Color your tabs. Useful when working with many tabs",
	"version": "0.0.3",
	"publisher": "mondersky",
	"icon": "docs/tabscolor.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/mondersky/tabscolor-vscode"
	},
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"keywords": [
		"tab" , "color", "hightligh" , "background", "style"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tabscolor.none",
				"title": "none",
				"category": "functions"
			},
			{
				"command": "tabscolor.debugColors",
				"title": "debugColors",
				"category": "functions"
			},
			{
				"command": "tabscolor.debugEraseStorage",
				"title": "erase variables storage",
				"category": "functions"
			},
			{
				"command": "tabscolor.repatch",
				"title": "repatch",
				"category": "functions"
			},
			{
				"command": "tabscolor.removePatch",
				"title": "remove patch",
				"category": "functions"
			},
			{
				"command": "tabscolor.red",
				"title": "red",
				"category": "colors"
			},
			{
				"command": "tabscolor.green",
				"title": "green",
				"category": "colors"
			},
			{
				"command": "tabscolor.blue",
				"title": "blue",
				"category": "colors"
			},
			{
				"command": "tabscolor.orange",
				"title": "orange",
				"category": "colors"
			},
			{
				"command": "tabscolor.yellow",
				"title": "yellow",
				"category": "colors"
			},
			{
				"command": "tabscolor.salmon",
				"title": "salmon",
				"category": "colors"
			},
			{
				"command": "tabscolor.white",
				"title": "white",
				"category": "colors"
			},
			{
				"command": "tabscolor.black",
				"title": "black",
				"category": "colors"
			},
			{
				"command": "tabscolor.test",
				"title": "Hello World",
				"category": "Hello"
			},
			{
				"command": "tabscolor.clearTabsColors",
				"title": "clear all tabs colors",
				"category": "TabsColor"
			}
		],
		"menus": {
			"editor/title/context": [
				{
					"submenu": "colors",
					"group": "navigation",
					"title": "color"
				}
			],
			"colors": [
				{
					"command": "tabscolor.blue",
					"group": "navigation"
				},
				{
					"command": "tabscolor.salmon",
					"group": "navigation"
				},
				{
					"command": "tabscolor.green",
					"group": "navigation"
				},
				{
					"command": "tabscolor.red",
					"group": "navigation"
				},
				{
					"command": "tabscolor.orange",
					"group": "navigation"
				},
				{
					"command": "tabscolor.yellow",
					"group": "navigation"
				},
				{
					"command": "tabscolor.black",
					"group": "navigation"
				},
				{
					"command": "tabscolor.white",
					"group": "navigation"
				},
				{
					"command": "tabscolor.none",
					"group": "functions"
				}
			]
		},
		"submenus": [
			{
				"id": "colors",
